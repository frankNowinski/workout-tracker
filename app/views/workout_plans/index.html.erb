<div class="container" id="workout-plan-list">
  <h2><%= "#{current_user.user_name}'s" if current_user.name %> Workout Plans</h2><br>

  <% @workout_plans.each do |workout_plan| %>
    <h4><%= link_to "#{workout_plan.name} - #{workout_plan.created_date}", new_workout_plan_exercise_path(workout_plan) %></h4><br>
  <% end %>
</div>
